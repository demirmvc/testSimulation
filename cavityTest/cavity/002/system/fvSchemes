FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         backward;
}

gradSchemes
{
    default         Gauss cubic;
    grad(p)         Gauss cubic;
}

divSchemes
{
    default         none;
    div(phi,U)      Gauss cubic;
    div((nuEff*dev2(T(grad(U))))) Gauss cubic;
    div(U)		Gauss cubic;

}

laplacianSchemes
{
    default         Gauss cubic orthogonal;
}

interpolationSchemes
{
    default         cubic;
}

snGradSchemes
{
    default         orthogonal;
}


// ************************************************************************* //
